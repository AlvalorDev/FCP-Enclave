    <!-- President Dick Richardson -->
    
    <PawnKindDef ParentName="FCP_EnclavekindBase">
        <defName>FCP_Unique_Pawnkind_President_Richardson</defName>
        <label>President Richardson</label>
        <defaultFactionDef>FCP_Faction_Enclave</defaultFactionDef>
        <factionLeader>true</factionLeader>
        <combatPower>85</combatPower>
        <itemQuality>Excellent</itemQuality>
        <gearHealthRange>0.9~1.0</gearHealthRange>
        <apparelRequired>
            <li>FCP_Apparel_Enclave_Officer_Coat</li>
        </apparelRequired>
        <weaponMoney>1800~2400</weaponMoney>
        <weaponTags>
            <li MayRequire="Rick.FCP.EnergyWeapons">FCP_Enclave_Weapon</li>
        </weaponTags>
        <initialWillRange>7~8</initialWillRange>
        <initialResistanceRange>30~40</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Character_Backstory_Richardson_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Character_Backstory_Richardson_Adult</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
        <inventoryOptions>
            <skipChance>0.0</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>FCP_Currency_BoS_Scrip</thingDef>
                    <countRange>200~300</countRange>
                </li>
                <li>
                    <thingDef>MealSurvivalPack</thingDef>
                    <countRange>4~6</countRange>
                </li>
                <li>
                    <thingDef>MedicineIndustrial</thingDef>
                    <countRange>3~5</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

